<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Sklad.Views.Items.ChooseElements"
		    Title="Multi-select"
		    Padding="10,20,10,10">

    <StackLayout>
        <Label FontAttributes="Bold">Choose some elements</Label>
        <Button Text="Create new Item" Clicked="OnClick" />
        <Label x:Name="results" />
    </StackLayout>
</ContentPage>